{
	"manifest_version": 3,
	"name": "Domain Tab Title",
	"short_name": "domaintabtit",
	"description": "Set the current tab's domain name as the tab's title.",
	"version": "1.0.0",
	"icons": {
		"48": "icon_48.png"
	},
	"content_scripts": [{
		"matches": [ "http://*/*", "https://*/*" ],
		"js": [ "dtt.js" ]
	}]
}
